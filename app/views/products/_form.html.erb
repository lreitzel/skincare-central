<%= form_for @product do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br><br>
    <%= f.label :category %>
    <%= f.text_field :category %><br><br>
    <%= f.label :brand %>
    <%= f.text_field :brand %><br><br>
    <%= f.label :price %>
    <%= f.select :price, options_for_select(Product::PRICE) %><br><br>
    <%= f.submit %>
<% end %>